
config wifidog 'wifidog'
	option wifidog_enable '0'
	option deamo_enable '0'
	option gateway_httpport '80'
	option gateway_connmax '50'
	option gatewayport '2060'
	option check_interval '1800'
	option client_timeout '60'
	option gateway_interface 'br-lan'
	option gateway_eninterface 'eth0.2'
	option sxsd '1'
	option xxsd '2'
	option gateway_hostname 'portal.wietone.com'
	option gateway_path '/wifidoglogin/'
	option enable '1'
	option auth_server_hostname 'portal.wietone.com'
	option auth_server_port '80'
	option auth_server_path '/wifidoglogin/'
	option wired_passed '0'
	option pool_mode '0'
	option httpd_max_conn '200'

